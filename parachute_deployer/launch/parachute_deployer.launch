<?xml version="1.0"?>
<launch>

    <include file="$(find px4)/launch/posix_sitl.launch">
            <arg name="vehicle" value="iris_fpv_cam"/>
            <arg name="world" value="/home/nikopihl/.gazebo/worlds/aruco_world.world"/>
    </include>
    <include file="$(find mavros)/launch/px4.launch">
            <arg name="fcu_url" value="udp://:14540@127.0.0.1:14557"/>
    </include>

    <node name="offboard_controller" pkg="offboard_control" type="offboard_control.py" output="screen"/>

    <node name="aruco_landing" pkg="aruco_landing" type="aruco_landing.py" output="screen"/>

    <node name="parachute_deployer" pkg="parachute_deployer" type="parachute_deployer.py" output="screen"/>
</launch>
